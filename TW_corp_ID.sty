\def\@TWcorpIDhead#1#2#3#4#5{#1\footnotesize\hbox to\kopfbreite{\TWcorpID@reset\vbox{\hbox
{\footnotesize\rlap{\parbox[b]{\kopfbreite}{\raggedright#2\strut}}\hfill
\footnotesize\parbox[b]{\kopfbreite}{\centering#3\strut}\hfill
\llap{\footnotesize\parbox[b]{\kopfbreite}{\raggedleft#4\strut}}}\headrule}}#5}

\def\@TWcorpIDfoot#1#2#3#4#5{#1\footnotesize\hbox to\kopfbreite{\TWcorpID@reset\vbox{\footrule
\hbox{\footnotesize\rlap{\parbox[t]{\kopfbreite}{\raggedright#2\strut}}\hfill
\footnotesize\parbox[t]{\kopfbreite}{\centering#3\strut}\hfill
\llap{\footnotesize\parbox[t]{\kopfbreite}{\raggedleft#4\strut}}}}}#5}

\def\headrule{{\if@TWcorpIDplain\kopfliniendicke\plainkopfliniendicke\fi
\hrule\@height\kopfliniendicke\@width\kopfbreite \vskip-\kopfliniendicke}}

\def\footrule{{\if@TWcorpIDplain\fussliniendicke\plainfussliniendicke\fi
\vskip-0.3\normalbaselineskip\vskip-\fussliniendicke
\hrule\@width\kopfbreite\@height\fussliniendicke\vskip0.3\normalbaselineskip}}

