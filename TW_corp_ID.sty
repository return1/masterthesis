% Diese Definitionen der Kopf- und Fußzeilen folgen 
% im Wesentlichen den Definitionen im 
% fancyheadings.sty Paket von Piet van Oostrum, 
% Dept of Computer Science, University of Utrecht

\def\lkopf{\@ifnextchar[{\@xlkopf}{\@ylkopf}}
\def\@xlkopf[#1]#2{\gdef\@elkopf{#1}\gdef\@olkopf{#2}}
\def\@ylkopf#1{\gdef\@elkopf{#1}\gdef\@olkopf{#1}}

\def\ckopf{\@ifnextchar[{\@xckopf}{\@yckopf}}
\def\@xckopf[#1]#2{\gdef\@eckopf{#1}\gdef\@ockopf{#2}}
\def\@yckopf#1{\gdef\@eckopf{#1}\gdef\@ockopf{#1}}

\def\rkopf{\@ifnextchar[{\@xrkopf}{\@yrkopf}}
\def\@xrkopf[#1]#2{\gdef\@erkopf{#1}\gdef\@orkopf{#2}}
\def\@yrkopf#1{\gdef\@erkopf{#1}\gdef\@orkopf{#1}}

\def\lfuss{\@ifnextchar[{\@xlfuss}{\@ylfuss}}
\def\@xlfuss[#1]#2{\gdef\@elfuss{#1}\gdef\@olfuss{#2}}
\def\@ylfuss#1{\gdef\@elfuss{#1}\gdef\@olfuss{#1}}

\def\cfuss{\@ifnextchar[{\@xcfuss}{\@ycfuss}}
\def\@xcfuss[#1]#2{\gdef\@ecfuss{#1}\gdef\@ocfuss{#2}}
\def\@ycfuss#1{\gdef\@ecfuss{#1}\gdef\@ocfuss{#1}}

\def\rfuss{\@ifnextchar[{\@xrfuss}{\@yrfuss}}
\def\@xrfuss[#1]#2{\gdef\@erfuss{#1}\gdef\@orfuss{#2}}
\def\@yrfuss#1{\gdef\@erfuss{#1}\gdef\@orfuss{#1}}

